@using Core.Dto;

<div class="day-col-container">
    <table class="scheduler-inner-table">
        <tbody>
            @for (int i = 1; i <= 24; i++)
            {
                <tr><td class="cell-half-hour"></td></tr>
                <tr><td class="cell-hour"></td></tr>
            }
        </tbody>
    </table>



    <div class="event" style="--event-start: 72px; --event-height: 72px">
    </div>
</div>

@code {
    [Parameter]
    public List<EventBooking> Events { get; set; } = new List<EventBooking>();
}